<mvc:View
    controllerName="ui5.dashboardapp.controller.DashboardTabHome"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f"
    xmlns:w="sap.ui.integration.widgets"
    height="100%"
>
    <Title
        text="Monitoring"
        titleStyle="H2"
        class="sapUiSmallMarginBottom sapUiSmallMarginTop sapUiSmallMarginBegin"
    />
    <f:GridContainer>
        <f:layout>
            <f:GridContainerSettings
                rowSize="5rem"
                columnSize="5rem"
                gap="1rem"
            />
        </f:layout>
        <f:layoutS>
            <f:GridContainerSettings
                rowSize="5rem"
                columnSize="5rem"
                gap="0.5rem"
            />
        </f:layoutS>
        <w:Card manifest="{manifests>/component}">
            <w:layoutData>
                <f:GridContainerItemLayoutData
                    minRows="3"
                    columns="3"
                />
            </w:layoutData>
        </w:Card>
        <w:Card manifest="{manifests>/donut}">
            <w:layoutData>
                <f:GridContainerItemLayoutData
                    rows="3"
                    columns="3"
                />
            </w:layoutData>
        </w:Card>
        <w:Card manifest="{manifests>/component-defects}">
            <w:layoutData>
                <f:GridContainerItemLayoutData
                    minRows="3"
                    columns="5"
                />
            </w:layoutData>
        </w:Card>
        <w:Card manifest="{manifests>/component-notifications}">
            <w:layoutData>
                <f:GridContainerItemLayoutData
                    minRows="3"
                    columns="3"
                />
            </w:layoutData>
        </w:Card>
    </f:GridContainer>
    <Title
        text=""
        titleStyle="H2"
        class="sapUiSmallMarginBottom sapUiSmallMarginTop sapUiSmallMarginBegin"
    />

    <f:GridContainer>
        <f:layout>
            <f:GridContainerSettings
                rowSize="5rem"
                columnSize="5rem"
                gap="1rem"
            />
        </f:layout>
        <f:layoutS>
            <f:GridContainerSettings
                rowSize="5rem"
                columnSize="5rem"
                gap="0.5rem"
            />
        </f:layoutS>
        <w:Card manifest="{manifests>/stackedColumn}">
            <w:layoutData>
                <f:GridContainerItemLayoutData
                    rows="5"
                    columns="4"
                />
            </w:layoutData>
        </w:Card>
    </f:GridContainer>

    <!-- <mvc:XMLView viewName="ui5.dashboardapp.view.HelloPanel" /> -->
    <!-- <mvc:XMLView viewName="ui5.dashboardapp.view.InvoiceList" /> -->
</mvc:View>
